{
  "name": "brewman-api",
  "version": "0.0.4",
  "description": "Brewman Api",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "codegen": "npx swag-ts ./swagger.config.json",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "build": "tsc",
    "prepare": "npm run lint && npm run format && rm -rf dist && npm run build",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Premier-Systems/brewman-api"
  },
  "keywords": [
    "brewman"
  ],
  "author": "Premier Systems",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "swagger-typescript": "^6.2.7",
    "typescript": "^4.4.3",
    "vitest": "^0.21.1"
  }
}
